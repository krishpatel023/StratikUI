{
  "name": "registry",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@9.8.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx @biomejs/biome lint ./src",
    "lint:fix": "npx @biomejs/biome lint ./src --write",
    "generate:docs": "tsx src/scripts/generateDocs.ts",
    "generate:search": "tsx src/scripts/generateSearchData.ts",
    "generate:sidebar": "tsx src/scripts/generateSidebarData.ts",
    "generate:data": "tsx src/scripts/generateDocs.ts && tsx src/scripts/generateSidebarData.ts && tsx src/scripts/generateSearchData.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5"
  }
}