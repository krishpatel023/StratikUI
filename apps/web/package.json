{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@9.8.0",
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && next build",
    "start": "next start",
    "lint": "npx @biomejs/biome lint ./src --skip=suspicious/noArrayIndexKey --skip=suspicious/noExplicitAny",
    "lint:fix": "npx @biomejs/biome lint ./src --write && npm run format",
    "lint:fix-unsafe": "npx @biomejs/biome lint ./src --write --unsafe && npm run format",
    "format": "npx @biomejs/biome format ./src --write",
    "check": "npx @biomejs/biome check ./src",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/mdx": "^2.0.13",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.23",
    "fuse.js": "^7.0.0",
    "mdx-annotations": "^0.1.4",
    "next": "14.1.3",
    "react": "^18",
    "react-aria-components": "^1.2.1",
    "react-dom": "^18",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@shikijs/transformers": "^1.3.0",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "braces": "^3.0.3",
    "postcss": "^8",
    "shiki": "^1.3.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-react-aria-components": "^1.1.3",
    "tsx": "^4.15.4",
    "typescript": "^5"
  }
}